{
    "name": "agenceink/craft-mailerlite",
    "description": "An easy tool to subscribe users to your MailerLite list",
    "type": "craft-plugin",
    "license": "mit",
    "version": "1.0.0",
    "support": {
        "email": "support@agenceink.com",
        "issues": "https://github.com/agenceink/craft-mailerlite.git/issues?state=open",
        "source": "https://github.com/agenceink/craft-mailerlite.git",
        "docs": "https://github.com/agenceink/craft-mailerlite.git",
        "rss": "https://github.com/agenceink/craft-mailerlite.git/releases.atom"
    },
    "authors": [
        {
          "name": "Agence ink",
          "homepage": "https://agenceink.com"
        }
    ],
    "require": {
        "php": ">=8.0.2",
        "craftcms/cms": "^4.4.0"
    },
    "require-dev": {
        "craftcms/ecs": "dev-main",
        "craftcms/phpstan": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "agenceink\\craftmailerlite\\": "src/"
        }
    },
    "extra": {
        "handle": "mailerlite",
        "name": "MailerLite",
        "developer": "Agence ink",
        "documentationUrl": "https://github.com/agenceink/craft-mailerlite.git"
    },
    "scripts": {
        "check-cs": "ecs check --ansi",
        "fix-cs": "ecs check --ansi --fix",
        "phpstan": "phpstan --memory-limit=1G"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.0.2"
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}

